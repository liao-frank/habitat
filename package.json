    "@types/uuid": "^8.3.4",
    "buffer": "^6.0.3",
    "concurrently": "^7.0.0",
    "events": "^3.3.0",
    "https-browserify": "^1.0.0",
    "nodemon": "^2.0.15",
    "parcel": "^2.3.2",
    "path-browserify": "^1.0.1",
    "prettier": "^2.6.0",
    "process": "^0.11.10",
