  "scripts": {
    "demo": "yarn demo-build && concurrently \"nodemon ./build/index.js\" \"npm:demo-watch\"",
    "demo-build": "parcel build ./demo/index.ts --dist-dir ./build",
    "demo-watch": "parcel watch ./demo/index.ts --dist-dir ./build",
